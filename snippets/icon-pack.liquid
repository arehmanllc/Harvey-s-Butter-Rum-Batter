  {% when 'next' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="23" viewBox="0 0 22 23" fill="none">
      <path d="M8.25 17.1448L13.75 11.6448L8.25 6.14484" stroke="currentColor" stroke-width="1.46667" stroke-linecap="round" stroke-linejoin="round"></path>
    </svg>
  {%- when 'plus-circle' -%}
