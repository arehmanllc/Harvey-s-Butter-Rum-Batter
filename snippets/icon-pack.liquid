      <path d="M11 7.5C11.2761 7.5 11.5 7.27614 11.5 7C11.5 6.72386 11.2761 6.5 11 6.5C10.7239 6.5 10.5 6.72386 10.5 7C10.5 7.27614 10.7239 7.5 11 7.5Z" fill="white" stroke="currentColor"/>
    </svg>
  {% when 'tick' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="17" height="16" viewBox="0 0 17 16" fill="none">
      <path fill-rule="evenodd" clip-rule="evenodd" d="M8.75 16C13.1683 16 16.75 12.4183 16.75 8C16.75 3.58172 13.1683 0 8.75 0C4.33172 0 0.75 3.58172 0.75 8C0.75 12.4183 4.33172 16 8.75 16Z" fill="currentColor"/>
      <path fill-rule="evenodd" clip-rule="evenodd" d="M13.6996 6.29301C14.0901 5.90248 14.0901 5.26932 13.6996 4.8788C13.3091 4.48827 12.6759 4.48827 12.2854 4.8788L7.33564 9.82854L5.21432 7.70722C4.82379 7.3167 4.19063 7.3167 3.8001 7.70722C3.40958 8.09775 3.40958 8.73091 3.8001 9.12144L6.62853 11.9499C7.01905 12.3404 7.65222 12.3404 8.04274 11.9499L13.6996 6.29301Z" fill="white"/>
