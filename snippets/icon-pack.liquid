  {% when 'star-rating' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="15" viewBox="0 0 16 15" fill="none" class="icon icon-fill-star">
      <path d="M8.00005 0.0600586L10.0785 5.19938L15.6086 5.58792L11.363 9.15274L12.7024 14.5322L8.00005 11.5961L3.29777 14.5322L4.63711 9.15274L0.391602 5.58792L5.92164 5.19938L8.00005 0.0600586Z" fill="#333333"/>
